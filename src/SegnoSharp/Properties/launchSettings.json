{
  "profiles": {
    "SegnoSharp": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "SegnoSharp_CommonConfig__DataPath": "$(SolutionDir)..\\data",
        "SegnoSharp_CommonConfig__MusicPath": "$(SolutionDir)..\\music"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "sslPort": 8443,
      "httpPort": 8080,
      "publishAllPorts": true,
      "useSSL": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "SegnoSharp_CommonConfig__DataPath": "/var/segnosharp",
        "SegnoSharp_CommonConfig__MusicPath": "/var/music",
        "SegnoSharp_StreamingServer__Address": "host.docker.internal",
        "SegnoSharp_ConnectionStrings__SegnoSharpDatabaseSqlite": "Data Source=SegnoSharpDocker.db"
      }
    },
    "WSL": {
      "commandName": "WSL2",
      "launchBrowser": true,
      "launchUrl": "https://localhost:5001",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://localhost:5001;http://localhost:5000",
        "SegnoSharp_CommonConfig__DataPath": "../../data",
        "SegnoSharp_CommonConfig__MusicPath": "../../music",
        "SegnoSharp_ConnectionStrings__SegnoSharpDatabaseSqlite": "Data Source=SegnoSharpWsl.db"
      },
      "distributionName": ""
    }
  }
}